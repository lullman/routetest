<h2>Movies</h2>
<table>
  <tr>
    <th>Movie Title</th>
    <th>Released</th>
  </tr>
  <% @movies.each do |movie| %>
    <tr>
      <td><%= movie.movie_title %></td>
      <td><%= movie.released %></td>
      <td><%= link_to "Edit", edit_movie_path(movie) %></td>
      <td><%= link_to "View", movie_path(movie) %></td>
    </tr>
  <% end %>
</table>
</br>
<%= link_to "Edit All Movies", edit_all_movies_path %>
<%= link_to "New Movie", new_movie_path %>
